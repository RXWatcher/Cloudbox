global:
  checkNewVersion: false
  sendAnonymousUsage: false
entryPoints:
  web:
    address: :80
  websecure:
    address: :443
providers:
  docker:
    watch: true
    exposedByDefault: true
api:
  dashboard: true
log:
  level: debug
  filePath: /etc/traefik/traefik.log
accessLog:
  filePath: /etc/traefik/traefik_access.log
certificatesResolvers:
  leresolver:
    acme:
      email: {{ user.email }}
      storage: /etc/traefik/acme.json
      tlschallenge: true